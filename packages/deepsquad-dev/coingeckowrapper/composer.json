{
    "name": "deepsquad-dev/coingeckowrapper",
    "description": "A wrapper of the CoinGecko RESTful API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeepsquadDev\\Coingeckowrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeepsquadDev\\Coingeckowrapper\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "deepsquad-dev",
            "email": "martinsdaze@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0"
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    }
}
